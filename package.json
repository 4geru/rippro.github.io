    "next": "^12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-twitter-widgets": "^1.10.0",
    "react-youtube": "^7.13.1"
  },
  "devDependencies": {
    "@types/node": "17.0.0",
    "@types/react": "17.0.37",
    "eslint": "8.4.1",
    "eslint-config-next": "^12.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "typescript": "4.5.4"
  }
}
