{
  "name": "rippro.github.io",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start"
  },
  "lint-staged": {
